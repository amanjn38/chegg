#include <stdio.h>

int binarySearch(int array[], int x,int y, int low, int high) {
  // Repeat until the pointers low and high meet each other
  while (low <= high) {
    int mid = low + (high - low) / 2;

    if (array[mid].x == x && array[mid].y == y)
      return mid;

    if (array[mid] < x)
      low = mid + 1;

    else
      high = mid - 1;
  }

  return -1;
}

int main(void) {
  int n = sizeof(array) / sizeof(array[0]);
